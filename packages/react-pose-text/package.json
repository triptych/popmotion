{
  "name": "react-pose-text",
  "version": "3.1.0",
  "description": "Easily animate characters and words with React Pose",
  "main": "./lib/index.js",
  "module": "./dist/react-pose-text.es.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "rollup -c && tsc --emitDeclarationOnly && npm run measure",
    "watch": "rollup -c -w",
    "measure": "gzip -c dist/react-pose-text.js | wc -c",
    "prettier": "prettier ./src/* --write",
    "prepublishOnly": "npm run prettier && npm run build"
  },
  "repository": "https://github.com/Popmotion/popmotion/tree/master/packages/react-pose-text",
  "keywords": [
    "animation",
    "dom",
    "declarative",
    "popmotion",
    "react",
    "react animation"
  ],
  "author": "Matt Perry",
  "license": "GPLv3",
  "presets": [
    "env"
  ],
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-pose": "^3.1.0"
  },
  "devDependencies": {
    "@types/react": "^16.3.16",
    "@types/jest": "^23.1.1",	
    "jest": "^23.1.0",	
    "jest-cli": "^23.1.0",
    "prettier": "1.11.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "dependencies": {
    "hey-listen": "^1.0.5"
  },
  "unpkg": "./dist/react-pose-text.js",
  "prettier": {
    "parser": "typescript",
    "singleQuote": true
  },
  "private": false
}
